<p id="notice"><%= notice %></p>

<div>

<%= form_tag(users_path, method: "get") do %>
  <%= text_field_tag(:first_name, params[:first_name], placeholder: 'FirstName') %>
  <%= text_field_tag(:last_name, params[:last_name], placeholder: 'LastName') %>
  <%= text_field_tag(:url, params[:url], placeholder: 'URL') %>
  <%= submit_tag("Search") %>
<% end %>



</div>
<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>emails</th>
    </tr>
  </thead>

  <tbody>
    <% @emails.each do |email| %>
      <tr>
        <td><%= email %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>
